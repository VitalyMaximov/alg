#include <stdio.h>
#include <ctype.h>
#include <string.h>
#define SZ 50
#define T char
#define true 1 == 1
#define false 1 != 1
typedef int boolean;
boolean sort(T* str) {

int buf[strlen(str)], idx = 0;
int init = 0;
while(strlen(str)>init)
buf[init++] = -1;


  for(unsigned int i = 0; i < strlen(str); i++) {

  if (str[i] == ')' || str[i] == '}' || str[i] == ']')  { //buf[]

  char c;
  int j = i;                    // position index exclusion
  if(str[i] == ')') c = '(';
    if(str[i] == '}') c = '{';
      if(str[i] == ']') c = '[';
  while(str[j] != c && j != buf[idx] && j >= 0){ // if there is a matching parenthesis
      j--;
  }
  if (str[j] == c)
  buf[idx++] = j;
 else
 return false;
  }
  }
  return true;
  } 


int main()
{
    T m[SZ];
    int i = 0;

char c;
while ((c = getchar()) != '\n' && i < SZ  ){
m[i] = c;
i++;
}

    for (i = 0; i < strlen(m); ++i ){
        printf("%c", m[i]);
    }
    if(sort(m))
   printf("- true");
   else
   printf("- false");
     
    return 0;
}
